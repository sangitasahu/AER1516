<launch>
	<arg name="quad" default="SQ01s"/>
	<arg name="world_name" default="$(find acl_sim)/user_worlds/cylinders.sdf"/>
	
	<!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
